		<dependency>
			<groupId>de.dlr-se.emir.epd</groupId>
			<artifactId>EPDModel</artifactId>
			<version>${epd.version}</version>
		</dependency>
		<dependency>
			<groupId>de.dlr-se.emir.epd</groupId>
			<artifactId>VesselEditor</artifactId>
			<version>${epd.version}</version>
		</dependency>

		<dependency>
			<groupId>de.dlr-se.emir.epd</groupId>
			<artifactId>SimpleNMEASensor</artifactId>
			<version>${epd.version}</version>
		</dependency>
		<dependency>
			<groupId>de.dlr-se.emir.epd</groupId>
			<artifactId>AISViewer</artifactId>
			<version>${epd.version}</version>
		</dependency>
	</dependencies>
