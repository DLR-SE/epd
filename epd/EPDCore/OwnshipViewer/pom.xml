	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>de.dlr-se.emir.epd</groupId>
		<artifactId>EPDCore</artifactId>
		<version>4.3.0</version>
	</parent>
	<artifactId>OwnshipViewer</artifactId>
	<name>Ownship Information Views</name>
