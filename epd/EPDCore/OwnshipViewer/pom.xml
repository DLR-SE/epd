			<groupId>de.dlr-se.emir.epd</groupId>
			<artifactId>MapView</artifactId>
			<version>${epd.version}</version>
		</dependency>
		<dependency>
			<groupId>de.dlr-se.emir.model.domain.maritime</groupId>
			<artifactId>Vessel</artifactId>
			<version>${model.version}</version>
